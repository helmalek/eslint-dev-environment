{

	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},

	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},

	"rules": {
		"semi": "warn",
		"curly": "error",
		"quotes": ["error", "double"],
		"no-empty-function": "error",
		"no-unused-vars": "warn"
	}
}
